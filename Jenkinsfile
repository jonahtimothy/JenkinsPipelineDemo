pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
            
        }
        stage('Build') {
            steps {
                echo 'BUILDING'
            }
            
        }
        stage('Deploy') {
            steps {
                echo 'DEPLOYING'
            }
            
        }
        stage('Test') {
            steps {
                echo 'TESTING'
            }
            
        }
        stage('Release') {
            steps {
                echo 'RELEASE'
            }
            
        }
    }
}
